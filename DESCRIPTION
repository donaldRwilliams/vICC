Depends: R (>= 4.0.0)
Imports: 
  coda (>= 0.19-4),
  ggplot2,
  methods,
  nlme,
  Rdpack (>= 0.11-1),
  rjags (>= 4-10)
Encoding: UTF-8
LazyData: true
RoxygenNote: 7.1.1
